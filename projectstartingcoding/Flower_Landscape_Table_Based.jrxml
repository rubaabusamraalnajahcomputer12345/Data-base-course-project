		<queryString language="SQL">
			<![CDATA[SELECT
    L."Lawyer_id",
    L."FName",
    L."LName",
    COALESCE(C.case_count, 0) AS case_count,
    COALESCE(cl.client_count, 0) AS client_count,
    COALESCE(B.total_expenses, 0) AS total_expenses
FROM
    lawpro.Lawyer AS L
LEFT JOIN (
    SELECT "Lawyer_id", COUNT(DISTINCT "Case_id") AS case_count
    FROM lawpro.Case
    GROUP BY "Lawyer_id"
) AS C ON L."Lawyer_id" = C."Lawyer_id"

LEFT JOIN (
    SELECT "Lawyer_id", COUNT(DISTINCT "Client_id") AS client_count
    FROM lawpro.Client
    GROUP BY "Lawyer_id"
) AS cl ON L."Lawyer_id" = cl."Lawyer_id"

LEFT JOIN (
    SELECT "Lawyer_id", COALESCE(SUM("Expenses"), 0) AS total_expenses
    FROM lawpro.Billing
    GROUP BY "Lawyer_id"
) AS B ON L."Lawyer_id" = B."Lawyer_id";]]>
		</queryString>
